{
  "name": "micvital/block",
  "description": "A module to create small blocks of content to display anywhere on the site.",
  "type": "asgard-module",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.0",
    "micvital/core-module": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3",
    "orchestra/testbench": "3.5.*",
    "fzaninotto/faker": "~1.5",
    "friendsofphp/php-cs-fixer": "^2.7"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Block\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-2.0": "2.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
